# Docker Development Environment
# These values are used when running with Docker Compose

PORT=3001
NODE_ENV=development

# Database Configuration (matches docker-compose.yml)
DB_HOST=postgres
DB_PORT=5432
DB_NAME=pulltabs
DB_USER=pulltabs_user
DB_PASSWORD=pulltabs_password

# JWT Configuration
JWT_SECRET=docker_dev_jwt_secret_key_min_32_characters
JWT_EXPIRES_IN=24h

# CORS Configuration
CLIENT_URL=http://localhost:5173

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100