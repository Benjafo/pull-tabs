{
  "name": "pull-tabs-server",
  "version": "1.0.0",
  "description": "Backend server for Pull Tabs game simulator",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "express": "^4.21.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cookie-parser": "^1.4.8",
    "sequelize": "^6.37.7",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "express-validator": "^7.2.0",
    "helmet": "^8.0.0",
    "express-rate-limit": "^7.5.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.6",
    "@types/express": "^5.0.0",
    "@types/cors": "^2.8.17",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/cookie-parser": "^1.4.7",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.18.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3",
    "tsx": "^4.19.2"
